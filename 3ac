// assgn arithmatic
#include <stdio.h>

int main() {
    int x = 5, y = 3;
    int temp = x + y; // intermediate variable to hold the result
    printf("t1 = x + y\n");
    printf("temp = t1\n");
    return 0;
}


























//arith
#include <stdio.h>

int main() {
    int a = 2, b = 3, c = 4;
    int t = b * c; // intermediate variable to hold the product of b and c
    int t1 = a + t; // result of the arithmetic expression
    printf("t = b * c\n");
    printf("t1 = a + t\n");
    return 0;
}


































//if else
#include <stdio.h>

int main() {
    int x = 5, y = 3;
    int t1 = x + y; // intermediate variable to hold the sum of x and y
    printf("if t1 > 10 goto L1\n");
    printf("goto L2\n");
    printf("L1: printf(\"t1 is greater than 10\\n\")\n");
    printf("goto L3\n");
    printf("L2: printf(\"t1 is less than or equal to 10\\n\")\n");
    printf("L3:\n");
    return 0;
}
































//for while
#include <stdio.h>

int main() {
    int i = 1, sum = 0;
    printf("L1:\n"); // loop label
    printf("if i > 10 goto L2\n"); // conditional statement
    printf("t = sum + i\n"); // update statement
    printf("sum = t\n"); // update statement
    printf("i = i + 1\n"); // update statement
    printf("goto L1\n"); // unconditional jump
    printf("L2:\n"); // exit label
    printf("printf(\"sum = %d\\n\", sum)\n"); // output statement
    return 0;
}











































//procedure call

#include <stdio.h>

int add(int x, int y) {
    return x + y;
}

int main() {
    int a = 5, b = 3, c;
    c = add(a, b);
    printf("c = %d\n", c);
    return 0;
}

/* 3 ac compiler generates for the prog 
t1 = a + b
param t1
param a
call add, 2
c = return
printf("c = %d\n", c) */
